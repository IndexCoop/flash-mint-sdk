{
  "name": "@indexcoop/flash-mint-sdk",
  "version": "1.0.0",
  "description": "Helper functions for using Index's Flash Mint contracts.",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "build"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "npm run typegen && tsc",
    "hardhat": "npx hardhat node",
    "lint": "prettier -c . && eslint ./src",
    "lint:fix": "prettier -w . && eslint ./src --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "typegen": "tsc --emitDeclarationOnly || true",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "acp": "git add . && npm run commit && git push"
  },
  "keywords": [],
  "homepage": "https://github.com/IndexCoop/flash-mint-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/IndexCoop/flash-mint-sdk"
  },
  "bugs": {
    "url": "https://github.com/IndexCoop/flash-mint-sdk/issues"
  },
  "author": {
    "name": "0xModene",
    "email": "modene@indexcoop.com",
    "url": "https://indexcoop.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@ethersproject/wallet": "^5.7.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-jest": "^28.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "hardhat": "^2.12.4",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "semantic-release": "^18.0.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/abstract-signer": "^5.6.2",
    "@ethersproject/bignumber": "^5.6.2",
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/units": "^5.6.1",
    "@tenderly/hardhat-tenderly": "^1.5.0",
    "axios": "^0.27.2"
  }
}
